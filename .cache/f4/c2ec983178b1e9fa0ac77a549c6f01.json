"components/menu/menu.js","original":{"line":69,"column":26}},{"generated":{"line":92,"column":51},"source":"components/menu/menu.js","original":{"line":69,"column":15}}],"sources":{"components/menu/menu.js":"import React, { useState, useEffect } from 'react'\nimport { NavLink, withRouter } from 'react-router-dom'\nimport { FaSignOutAlt, FaHeadphones, FaSpotify } from 'react-icons/fa'\nimport MoodMojoLogo from '~/lib/images/moodmojo-logo.svg'\nimport SpotifyLogo from '~/lib/images/Spotify_Logo_RGB_White.png'\nimport './menu.scss'\n\nconst SPOTIFY_CLIENT_ID = 'e7e008fe415447bba73e3e6ce86e8ecb'\nconst queryObj = {\n\tclient_id: SPOTIFY_CLIENT_ID,\n\tresponse_type: 'token',\n\tredirect_uri: 'http://localhost:4000/spotify-callback',\n\tscope: 'playlist-modify-public playlist-modify-private'\n}\n\nconst Menu = ({ history }) => {\n\tconst queryString = Object.entries(queryObj).map(([key, val]) => `${key}=${val}`).join('&')\n\tconsole.log(queryString)\n\tconst authorizeLink = `https://accounts.spotify.com/authorize?${queryString}`\n\tconst [storageIndex, setStorageIndex] = useState(0)\n\n\tconst handleStorage = (e) => {\n\t\tif (e.key == 'accessToken')\n\t\t\thistory.push('/generator')\n\t\telse if (!e.key)\n\t\t\thistory.push('/')\n\t}\n\n\tuseEffect(() => {\n\t\tsetStorageIndex(app.storageEvent.subscribe(handleStorage))\n\n\t\treturn () => {\n\t\t\tapp.storageEvent.unsubscribe(storageIndex)\n\t\t};\n\t}, [])\n\n\treturn (\n\t\t<nav className=\"menu\">\n\t\t\t<div>\n\t\t\t\t<a href=\"https://www.spotify.com/\" target=\"_blank\" id=\"spotifyLink\">\n\t\t\t\t\t<img src={SpotifyLogo} />\n\t\t\t\t</a>\n\t\t\t\t<NavLink to=\"/\" id=\"homeLink\">\n\t\t\t\t\t<img src={MoodMojoLogo} />\n\t\t\t\t</NavLink>\n\t\t\t</div>\n\t\t\t{\n\t\t\t\tlocalStorage.getItem(\"accessToken\") ?\n\t\t\t\t\t<div>\n\t\t\t\t\t\t<NavLink to=\"/generator\" id=\"homeLink\">\n\t\t\t\t\t\t\t<button><FaHeadphones /> <span>Generator</span></button>\n\t\t\t\t\t\t</NavLink>\n\t\t\t\t\t\t<a onClick={() => { localStorage.clear(), window.dispatchEvent(new Event('storage')); }}>\n\t\t\t\t\t\t\t<button><FaSignOutAlt /> <span>Logout</span></button>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</div>\n\t\t\t\t\t:\n\t\t\t\t\t<div>\n\t\t\t\t\t\t<a href={authorizeLink}>\n\t\t\t\t\t\t\t<button><FaSpotify /> <span>Get Started</span></button>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</div>\n\t\t\t}\n\n\t\t</nav>\n\t)\n}\n\nexport default withRouter(Menu)"},"lineCount":null}},"error":null,"hash":"b246b3f6161094f890518ce1336cc7e0","cacheData":{"env":{}}}